    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "uglify-js": "^3.1.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.6.2",
    "webpack": "^2.7.0",
    "webpack-dev-middleware": "^1.12.0",
