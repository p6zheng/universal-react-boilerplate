{
  "name": "universal-react-boilerplate",
  "version": "1.0.0",
  "description": "Universal React Boilerplate",
  "scripts": {
    "start": "npm run build && npm run prod",
    "dev": "cross-env SERVER_PORT=3000 nodemon --watch server --exec babel-node index.js",
    "prod": "NODE_ENV=production babel-node index.js",
    "build": "webpack --config webpack.config.prod.js",
    "test": "NODE_ENV=test nodemon --exec 'mocha -R min --compilers js:babel-core/register --timeout 15000 --recursive'",
    "clean": "rimraf dist",
    "lint": "./node_modules/.bin/eslint client server"
  },
  "repository": {},
  "author": "Pengfei Zheng",
  "devDependencies": {
    "babel-cli": "^6.26.0",
