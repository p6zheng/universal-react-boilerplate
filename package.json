    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.1",
    "express-session": "^1.15.6",
    "material-ui": "^1.0.0-beta.36",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
