    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
